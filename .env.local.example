# ======================================
# Media Project Manager - Local Settings (EXAMPLE)
# ======================================
# Kopiere diese Datei als .env.local und passe die Werte an
# cp .env.local.example .env.local

# Application Settings
VITE_APP_VERSION=1.1.0
VITE_DEV_MODE=true
VITE_LOG_LEVEL=debug

# API Endpoints
VITE_SUNO_API_URL=https://api.suno.ai/v1
VITE_YOUTUBE_API_URL=https://www.googleapis.com/youtube/v3
VITE_DREAMEDIT_API_URL=https://api.dreamedit.ai/v1
VITE_DISTROKID_API_URL=https://api.distrokid.com/v1
VITE_ELEVENLABS_API_URL=https://api.elevenlabs.io/v1

# Feature Flags
VITE_ENABLE_FILE_DROP=true
VITE_ENABLE_SMART_PROMPTS=true
VITE_ENABLE_TOOL_BUTTONS=true
VITE_ENABLE_TASK_TRACKER=true

# API Services
VITE_ENABLE_SUNO_API=true
VITE_ENABLE_YOUTUBE_API=true
VITE_ENABLE_DREAMEDIT_API=true
VITE_ENABLE_DISTROKID_API=true
VITE_ENABLE_ELEVENLABS_API=true

# Development Settings
VITE_ENABLE_HOT_RELOAD=true
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_ERROR_OVERLAY=true

# Performance Settings
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# Mock Data
VITE_USE_MOCK_API=false
VITE_MOCK_DELAY=500

# Analytics
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
